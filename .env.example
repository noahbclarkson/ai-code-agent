# Single API key (use either this OR GEMINI_API_KEYS)
GEMINI_API_KEY=your_api_key_here

# Multiple API keys for rotation (comma-separated)
# Uncomment and use this instead of GEMINI_API_KEY for rate limit avoidance
# GEMINI_API_KEYS=key1,key2,key3

# Gemini model to use (default: gemini-2.5-pro)
GEMINI_MODEL=gemini-2.5-pro

# Token character limit for codebase reports (default: 200000)
TOKEN_CHAR_LIMIT=200000

# Path to codebase_viewer executable
# Use forward slashes for Windows paths in .env files
# Windows: C:/path/to/codebase_viewer.exe
# Linux/Mac: /path/to/codebase_viewer
CODEBASE_VIEWER_PATH=C:/path/to/codebase_viewer.exe
